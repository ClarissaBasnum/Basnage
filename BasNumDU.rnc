    tei_att.global.rendition.attribute.rendition,
    
    ## (développé) gives an expanded form of information presented more concisely in the dictionary In this case we expand the grammatical elements coded under <pos>
    attribute expand {
      
      ##
      "Verbe"
      | 
        ##
        " Substantif"
      | 
        ##
        " Adverbe"
      | 
        ##
        " Adjectif"
      | 
        ##
        " Participe_passe"
      | 
        ##
        " Participe_passe Adjectif"
      | 
        ##
        " Pronom_demonstratif"
      | 
        ##
        " Pronom"
      | 
        ##
