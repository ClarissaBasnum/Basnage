  element usg {
    tei_macro.paraContent,
    tei_att.global.attributes,
    tei_att.lexicographic.attributes,
    
    ## classe les informations sur l'usage en utilisant n'importe quelle typologie adaptée
    ## Exemple de valeurs possibles: 1] geo (geographic); 2] time; 3] dom (domain); 4] register; 5] style; 6] plev (preference level); 7] lang (language); 8] gram (grammatical); 9] syn (synonym); 10] hyper (hypernym); 11] colloc (collocation); 12] comp (complement); 13] obj (object); 14] subj (subject); 15] verb; 16] hint
    attribute type {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
